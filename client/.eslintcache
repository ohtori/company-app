y(quantity, sale, goodListState);\n\n  useEffect(() => {\n    fetch(`/get-goods?${reqParams}`)\n      .then(response => response.json())\n      .then(result => {\n        setGoodListState((prev: IGoodListState) => {\n          return {...prev, goods: result}\n        });\n        setLoading(false);\n        setIsFilter(false);\n      })\n      .catch(e => setLoading(false));\n  }, [goodListState.category, goodListState.searchValue, goodListState.page, isFilter, setGoodListState, setIsFilter]);\n  \n\n  return (\n    <>\n      <h1>{ title || loadableTitle.category || ' ' }</h1>\n      <div className=\"resize\">\n      {loading \n        ? <PageLoader />\n        : (goodListState.goods.length \n          ? goodListState.goods.map((good: any) => {\n            return <GoodInList good={good} key={good._id} />\n          })\n          : <p>{'По вашему запросу товаров не найдено.'}</p>\n        )\n      }\n      </div>\n    </>\n  );\n}","C:\\Users\\Ohtori\\company-app\\client\\src\\helpers\\authValidate.ts",[],"C:\\Users\\Ohtori\\company-app\\client\\src\\helpers\\createRequestQuery.ts",[],"C:\\Users\\Ohtori\\company-app\\client\\src\\components\\SidebarCategories.tsx",[],"C:\\Users\\Ohtori\\company-app\\client\\src\\components\\GoodInList.tsx",[],"C:\\Users\\Ohtori\\company-app\\client\\src\\components\\basket\\BasketGood.tsx",["123"],"C:\\Users\\Ohtori\\company-app\\client\\src\\components\\filter\\SidebarFilter.tsx",[],"C:\\Users\\Ohtori\\company-app\\client\\src\\components\\filter\\SidebarSelect.tsx",[],"C:\\Users\\Ohtori\\company-app\\client\\src\\components\\filter\\SidebarCheckbox.tsx",[],"C:\\Users\\Ohtori\\company-app\\client\\src\\components\\filter\\PriceFilter.tsx",[],"C:\\Users\\Ohtori\\company-app\\client\\src\\components\\basket\\BasketForm.tsx",[],"C:\\Users\\Ohtori\\company-app\\client\\src\\helpers\\basketFormValidate.ts",[],"C:\\Users\\Ohtori\\company-app\\client\\src\\components\\Good.tsx",["124"],"import { useContext, useEffect, useState } from \"re